{
  "manifest_version": 3,
  "name": "Twitter Logo Changer",
  "version": "1.3",
  "description": "Replace Twitter logo to the classic",
  "action": {
    "default_icon": {
      "48": "./PNG/blue.png"
    }
  },
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "48": "./PNG/blue.png"
  }
}
